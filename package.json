{
  "name": "yet-another-logger",
  "version": "0.1.1",
  "description": "Manage logs with bunyan and loggly.",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielyaa5/google-flights-api.git"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --ignore-pattern build/* --fix",
      "git add"
    ]
  },
  "keywords": [
    "logs",
    "bunyan",
    "loggly",
    "logger",
    "javascript"
  ],
  "author": "Daniel Yakobian",
  "license": "MIT",
  "homepage": "https://github.com/danielyaa5/yet-another-logger",
  "dependencies": {
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "bunyan-loggly": "^1.2.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "lint-staged": "^4.0.4",
    "prettier": "^1.6.0"
  },
  "scripts": {
    "build": "rm -rf build/* && babel src -d build",
    "docs": "jsdoc2md --configure .jsdoc --template README.hbs --files ./src/LogManager.js > README.md",
    "prepublish": "npm run build && npm run docs",
    "precommit": "lint-staged && npm run build && npm run docs && git add .",
    "lint-js": "eslint --ignore-path .gitignore --quiet --ignore-pattern \"!**/.*\" .",
    "lint": "npm run lint-js",
    "fix-js": "npm run lint-js -- --fix",
    "fix": "npm run fix-js"
  }
}
